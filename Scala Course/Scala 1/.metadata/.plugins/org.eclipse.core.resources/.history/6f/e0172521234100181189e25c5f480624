package week2
import math.abs


object worksheet2 {
  println("Fixed Point Function")                 //> Fixed Point Function
  
  val tolerance = 0.0001                          //> tolerance  : Double = 1.0E-4
  def isCloseEnough(x:Double, y:Double) =
  		abs((x-y)/x)/x<tolerance          //> isCloseEnough: (x: Double, y: Double)Boolean
  
  
  
  
  
  
  
  
  
  
  
}